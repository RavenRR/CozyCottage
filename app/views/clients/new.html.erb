<h1> New Client </h1>

<%= form_for(@client) do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <br>
    <br>

    <%= f.label "Client's Image" %>
    <%= f.file_field :image %>
    <br>
    <br>

    <%= f.label :bio %>
    <%= f.text_field "bio" %>
    <br>
    <br>

    <%= f.submit "Submit" %>
    
    <% end %>