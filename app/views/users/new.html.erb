
    <div class="jumbotron text-center">
<h2> Signup <h2>
</div>

<%= render partial: "layouts/errors", locals: {object: @user} %>
<%= form_for(@user) do |f| %>

<%= f.label :username %>
<%= f.text_field :username %>
<br>
    <br>
<%= f.label :email %>
<%= f.email_field :email %>
<br>
    <br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
    <br>
<%= f.submit "Submit", :class => "btn btn-primary btn-sm" %>

<%= link_to "Back", root_path, :class => "btn btn-primary btn-sm" %>
</div>
<% end %>
